const Realm = require('realm');

// Define your models and their properties
const MoodSchema = {
    name: 'Mood',
    primaryKey: 'id',
    properties: {
        id: 'int',
        mainMood: 'int',
        moods: 'int[]',
        note: 'string'
    }
};


Realm.open({ schema: [MoodSchema] })
    .then(realm => {
        // Create Realm objects and write to local storage
        realm.write(() => {
            const mymood = realm.create('Car', {
                mainMood: 1,
                moods: [1, 2],
                note: "This is0 the first Mood",
            });
        });

        realm.objects('Mood').filtered('mood = 1')
        console.log(realm)
        // Remember to close the realm when finished.
        realm.close();
    })
    .catch(error => {
        console.log(error);
    });